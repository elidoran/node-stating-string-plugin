    "test6": "nave use 6.10 npm test",
    "test7": "nave use 7.10 npm test",
    "test-all": "npm run test4 && npm run test6 && npm run test7",
    "coverage": "istanbul cover _mocha -- --compilers coffee:coffee-script/register -R spec test/lib",
